    <item name="android:layout_height">38dp</item>
    <item name="android:inputType">text</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:textColor">#0A0A0B</item>

    </style>

    <style name="recycler_1">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item >
        <item name="android:textSize">15sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/purple_500</item>
        <item name="android:gravity">start</item>


    </style>



    <style name="linear_layout_1">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item >
        <item name="android:textSize">18sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:inputType">text</item>
        <item name="android:textColor">#0A0A0B</item>
        <item name="android:gravity">start</item>


    </style>





    <style name="Titel_1">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#2196F3</item>
        <item name="android:gravity">start</item>
        <item name="android:layout_margin">10dp</item>



    </style>

    <style name="float_button_1">
        <item name="android:layout_margin">10dp</item>

        <item name="android:layout_gravity">bottom|end</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:layout_marginBottom">16dp</item>


        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>




    </style>


    <style name="linear_layout_horizontal_1">
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>




    </style>

    <style name="linear_layout_vertical_1">
        <item name="android:layout_margin">10dp</item>
